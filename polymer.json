{
    "entrypoint": "index.html",
    "shell": "src/my-app.html",
    "fragments": [
        "src/my-view1.html",
        "src/my-view2.html",
        "src/my-view-login.html",
        "src/my-view404.html"
    ],
    "sources": [
        "src/**/*",
        "resources/images/**/*"
    ],
    "extraDependencies": [
        "manifest.json",
        "index.php",
        "composer.json",
        "composer.lock",
        "browserconfig.xml",
        "bower_components/webcomponentsjs/*.js"
    ],
    "lint": {
        "rules": ["polymer-2"]
    },
    "builds": [
        {
            "name": "default",
            "preset": "es6-unbundled"
        }
    ]
}
