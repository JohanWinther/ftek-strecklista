{
    "entrypoint": "index.html",
    "shell": "client/my-app.html",
    "fragments": [
        "client/my-view1.html",
        "client/my-view2.html",
        "client/my-login.html",
        "client/my-view404.html"
    ],
    "sources": [
        "client/**/*",
        "resources/images/**/*"
    ],
    "extraDependencies": [
